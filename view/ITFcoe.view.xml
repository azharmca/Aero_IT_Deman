<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:viz="sap.viz.ui5.controls" xmlns:layout="sap.ui.layout" xmlns="sap.suite.ui.commons"
	xmlns:m="sap.m" xmlns:u="sap.ui.unified" controllerName="sap.Aero_IT_Demand.prototype.view.ITFcoe"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<m:Page>
		<m:customHeader>
			<m:Bar>
				<m:contentLeft>
					<m:Button type="Back" press="backToMain" class="backButton simpleBtn" />
				</m:contentLeft>
				<m:contentMiddle>
					<m:Label text="STATS" />
				</m:contentMiddle>
			</m:Bar>
		</m:customHeader>

		<m:content>
			<layout:Splitter width="100%" height="700px">
				<layout:contentAreas>
					<ChartContainer id="idChartContainer"
						showFullScreen="false" showPersonalization="false"
						autoAdjustHeight="true" showLegend="false" personalizationPress="attachPersonalizationPress"
						contentChange="attachContentChange">
						<dimensionSelectors>
							<m:MultiComboBox id="idoSelect1" selectionChange="handleSelectionChange"
								selectionFinish="onSelectItem" width="300px" items="{SelectModel>/List}"
								class="paddMultiCombo">
								<core:ListItem key="{SelectModel>description}"
									text="{SelectModel>title}" />
							</m:MultiComboBox>
						
						</dimensionSelectors>
						<content>
							<ChartContainerContent icon="sap-icon://vertical-bar-chart"
								title="Total">
								<content>
									<viz:VizFrame id="idoVizFrame4" uiConfig="{applicationSet:'fiori'}"
										vizType="column" width="100%">
									</viz:VizFrame>
								</content>
							</ChartContainerContent>
						</content>
					</ChartContainer>
				</layout:contentAreas>
			</layout:Splitter>
		</m:content>

		<!-- <m:footer> <m:Toolbar width="100%"> <m:TileContainer id="container" 
			tileDelete="handleTileDelete" tiles="1"> <m:StandardTile type="Monitor" number="5" 
			title="Azhar" /> </m:TileContainer> </m:Toolbar> </m:footer> -->

	</m:Page>
</core:View>
